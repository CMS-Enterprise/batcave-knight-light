        "drupal/auditfiles": "^4.0 || ^4.1",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/autologout": "^1.4",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/better_fields_report": "^1.0",
        "drupal/cer": "^5.0@beta",
        "drupal/clamav": "^2.0",
        "drupal/computed_field": "^4.0@alpha",
        "drupal/condition_path": "^2.0",
        "drupal/config_filter": "^2.6",
        "drupal/config_ignore": "^3.1",
        "drupal/config_overlay": "^2.1",
        "drupal/config_override_warn": "^1.3",
        "drupal/config_split": "^2.0",
